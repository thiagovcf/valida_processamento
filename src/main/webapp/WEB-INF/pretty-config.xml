		<pattern value="/visualizarCliente/codigoCliente=#{id:clienteBean.cliente.id}" />
		<view-id value="/pages/secure/cliente/incluirAlterarCliente.jsf" />
		<action>#{clienteBean.preencherVisualizar()}</action>
	</url-mapping>
	
	<url-mapping id="alterarCliente">
		<pattern value="/alterarCliente/codigoCliente=#{id:clienteBean.cliente.id}" />
		<view-id value="/pages/secure/cliente/incluirAlterarCliente.jsf" />
			<action>#{clienteBean.preencherAlterar()}</action>
	</url-mapping>
