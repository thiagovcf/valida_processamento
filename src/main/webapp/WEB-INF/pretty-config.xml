	<url-mapping id="visualizarUsuario">
		<pattern value="/visualizarUsuario/codigoUsuario=#{id:usuarioBean.usuario.id}" />
		<view-id value="/pages/secure/usuarios/usuario/incluirAlterarUsuario.jsf" />
		<action>#{usuarioBean.preencherVisualizar()}</action>
	</url-mapping>
	
	<url-mapping id="alterarUsuario">
		<pattern value="/alterarUsuario/codigoUsuario=#{id:usuarioBean.usuario.id}" />
		<view-id value="/pages/secure/usuarios/usuario/incluirAlterarUsuario.jsf" />
			<action>#{usuarioBean.preencherAlterar()}</action>
	</url-mapping>
	<!-- Fim Usuario -->
	
	<!-- Configuracao -->
	<url-mapping id="visualizarConfiguracao">
		<pattern value="/visualizarConfiguracao" />
		<view-id value="/pages/secure/configuracao/visualizarAlterarConfiguracao.jsf" />
		<action>#{configuracaoBean.preencherVisualizar()}</action>
	</url-mapping>
	
	<url-mapping id="alterarConfiguracao">
		<pattern value="/alterarConfiguracao" />
		<view-id value="/pages/secure/configuracao/visualizarAlterarConfiguracao.jsf" />
		<action>#{configuracaoBean.preencherAlterar()}</action>
	</url-mapping>
	<!-- Fim Configuracao -->
	
	<!-- Emissor -->
	<url-mapping id="incluirEmissor">
		<pattern value="/incluirEmissor" />
		<view-id value="/pages/secure/processamento/emissor/incluirAlterarEmissor.jsf" />
	</url-mapping>
	
	<url-mapping id="visualizarEmissor">
		<pattern value="/visualizarEmissor/codigoEmissor=#{id:emissorBean.emissor.id}" />
