<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.3 http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

	<url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/pages/login.jsf" />
	</url-mapping>
	
	<url-mapping id="login2">
		<pattern value="/" />
		<view-id value="/pages/login.jsf" />
	</url-mapping>
	
	<url-mapping id="negado">
		<pattern value="/negado" />
		<view-id value="/pages/secure/negado.jsf" />
	</url-mapping>
	
	<url-mapping id="inicio">
		<pattern value="/inicio" />
		<view-id value="/pages/secure/inicio.jsf" />
	</url-mapping>

	<url-mapping id="alterarSenha">
		<pattern value="/alterarSenha" />
		<view-id value="/pages/alterarSenha.jsf" />
		<action>#{usuarioBean.validaToken}</action>
	</url-mapping>
	
	<!-- Grupo -->
	<url-mapping id="listarGrupo">
		<pattern value="/listarGrupo" />
		<view-id value="/pages/secure/usuarios/grupo/listarGrupo.jsf" />
	</url-mapping>
	
	<url-mapping id="incluirGrupo">
		<pattern value="/incluirGrupo" />
		<view-id value="/pages/secure/usuarios/grupo/incluirAlterarGrupo.jsf" />
	</url-mapping>
	
	<url-mapping id="visualizarGrupo">
		<pattern value="/visualizarGrupo/codigoGrupo=#{id:grupoBean.grupo.id}" />
		<view-id value="/pages/secure/usuarios/grupo/incluirAlterarGrupo.jsf" />
		<action>#{grupoBean.preencherVisualizar()}</action>
	</url-mapping>
	
	<url-mapping id="alterarGrupo">
		<pattern value="/alterarGrupo/codigoGrupo=#{id:grupoBean.grupo.id}" />
		<view-id value="/pages/secure/usuarios/grupo/incluirAlterarGrupo.jsf" />
		<action>#{grupoBean.preencherAlterar()}</action>
	</url-mapping>
	<!-- Fim Grupo -->
	
	<!-- Usuario -->
	<url-mapping id="listarUsuario">
		<pattern value="/listarUsuario" />
		<view-id value="/pages/secure/usuarios/usuario/listarUsuario.jsf" />
	</url-mapping>
	
	<url-mapping id="incluirUsuario">
		<pattern value="/incluirUsuario" />
