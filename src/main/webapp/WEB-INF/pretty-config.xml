	<!-- Pasta -->
	<url-mapping id="incluirPasta">
		<pattern value="/incluirPasta" />
		<view-id value="/pages/secure/processamento/pastas/incluirAlterarPasta.jsf" />
		<action>#{emissorBean.init()}</action>
	</url-mapping>
	
	<url-mapping id="visualizarPasta">
		<pattern value="/visualizarPasta/codigoPasta=#{id:folderFtpBean.folderFtp.id}" />
		<view-id value="/pages/secure/processamento/pastas/incluirAlterarPasta.jsf" />
		<action>#{folderFtpBean.preencherVisualizar()}</action>
		<action>#{emissorBean.preencheEmissorAtivo()}</action>
	</url-mapping>
	
	<url-mapping id="alterarPasta">
		<pattern value="/alterarPasta/codigoPasta=#{id:folderFtpBean.folderFtp.id}" />
		<view-id value="/pages/secure/processamento/pastas/incluirAlterarPasta.jsf" />
		<action>#{folderFtpBean.preencherAlterar()}</action>
		<action>#{emissorBean.preencheEmissorAtivo()}</action>
	</url-mapping>
	
	<url-mapping id="listarPasta">
		<pattern value="/listarPasta" />
		<view-id value="/pages/secure/processamento/pastas/listarPasta.jsf" />
		<action>#{emissorBean.init()}</action>
	</url-mapping>
	<!-- Fim Pasta -->
	
	<!-- inicio Arquivo -->
	<url-mapping id="visualizarConfigurarArquivo">
		<pattern value="/codigoPasta=#{id:fileFolderFtpBean.folderFtp.id}/visualizarConfigurarArquivo" />
		<view-id value="/pages/secure/processamento/arquivo/incluirAlterarArquivo.jsf" />
		<action>#{fileFolderFtpBean.preencherVisualizar()}</action>
	</url-mapping>
	<url-mapping id="alterarConfigurarArquivo">
		<pattern value="/codigoPasta=#{id:fileFolderFtpBean.folderFtp.id}/alterarConfigurarArquivo" />
		<view-id value="/pages/secure/processamento/arquivo/incluirAlterarArquivo.jsf" />
		<action>#{fileFolderFtpBean.preencherAlterar()}</action>
	</url-mapping>
	<!-- Fim Arquivo -->
	
	<!-- inicio execucao-->
	<url-mapping id="execucaoManual">
		<pattern value="/execucaoManual" />
		<view-id value="/pages/secure/processamento/execucao.jsf" />
	</url-mapping>
	<!-- fim execucao-->
	
	<!-- inicio historico-->
	<url-mapping id="historico">
		<pattern value="/historico" />
		<view-id value="/pages/secure/processamento/historico.jsf" />
		<action>#{processamentoBean.init()}</action>
	</url-mapping>
	<!-- fim historico-->
	
	<!-- inicio processamento-->
	<url-mapping id="visualizarProcessamento">
		<pattern value="/visualizarProcesssamento/codigoProcessamento=#{id:processamentoBean.processamento.id}" />
		<view-id value="/pages/secure/processamento/visualizarProcessamento.jsf" />
		<action>#{processamentoBean.pesquisarEmissoresProcessados()}</action>
	</url-mapping>
	<!-- fim processamento-->
	
	<!-- Inicio check -->
	<url-mapping id="checkArquivo">
		<pattern value="/checkArquivo" />
		<view-id value="/pages/secure/dashboard/checkArquivo.jsf" />
	</url-mapping>
	<!-- Fim check -->
	
