	</url-mapping>
	
	<url-mapping id="visualizarVenda">
		<pattern value="/visualizarVenda/codigoPasta=#{id:transacaoBean.transacao.id}" />
		<view-id value="/pages/secure/transacao/incluirAlterarTransacao.jsf" />
		<action>#{transacaoBean.preencherVisualizar()}</action>
		<action>#{clienteBean.preencheClienteAtivo()}</action>
	</url-mapping>
		
	<url-mapping id="listarVendas">
		<pattern value="/listarVenda" />
		<view-id value="/pages/secure/transacao/listarTransacao.jsf" />
