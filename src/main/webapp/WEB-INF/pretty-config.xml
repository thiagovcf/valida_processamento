	<!-- cliente -->
	<url-mapping id="listarCliente">
		<pattern value="/listarCliente" />
		<view-id value="/pages/secure/cliente/listarCliente.jsf" />
	</url-mapping>
	
	<url-mapping id="incluirCliente">
		<pattern value="/incluirCliente" />
		<view-id value="/pages/secure/cliente/incluirAlterarCliente.jsf" />
	</url-mapping>
	
