	</url-mapping>
	
	<url-mapping id="listarAplicacao">
		<pattern value="/listarAplicacao" />
		<view-id value="/pages/secure/validaapp/listarAplicacao.jsf" />
	</url-mapping>
	
	<url-mapping id="atualizarVersaoApp">
		<pattern value="/AtualizarVersaoApp" />
		<view-id value="/pages/secure/validaapp/incluirAlterarAplicacao.jsf" />
	</url-mapping>-->
